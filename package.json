{
  "name": "pay",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "migrate": "ts-node ./node_modules/.bin/typeorm -d ./src/database/appdatasource.ts migration:run",
    "migrate:show": "ts-node ./node_modules/.bin/typeorm -d ./src/database/appdatasource.ts  migration:show",
    "start": "npm run build && node build/index.js",
    "build": "rimraf ./build && tsc",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -d ./src/database/appdatasource.ts"
  },
  "keywords": [
    "typescript",
    "node",
    "express",
    "typeorm",
    "learning"
  ],
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.15.11",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.13"
  }
}
